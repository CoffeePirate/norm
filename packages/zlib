fetch_source http://ftp.de.debian.org/debian/pool/main/z/zlib/zlib_1.2.7.dfsg.orig.tar.gz
fetch_source http://ftp.de.debian.org/debian/pool/main/z/zlib/zlib_1.2.7.dfsg-13.debian.tar.gz

do_undebian zlib-1.2.7 zlib_1.2.7.dfsg.orig.tar.gz zlib_1.2.7.dfsg-13.debian.tar.gz
do_compile_nognu
do_strip $PREFIX/lib/libz.*$LIBSUFFIX
